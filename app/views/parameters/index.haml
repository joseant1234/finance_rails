.card.no-margin
  .card-content
    %p.bold.medium PARAMETERS

    .overflow-x
      %table.centered
        %thead#table_header
          = render 'table_header'
        %tbody#parameters_index
          = render @parameters

    .top-space.center.pagination-from-table
      = will_paginate @parameters, renderer: MaterializePaginationCustomHelper::PaginateMaterailizeLinkRenderer

.modal#modalParameter