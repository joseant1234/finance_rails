<% if @provider.present? %>
  $('.country-select').val("<%= @provider.country_id %>").change()
  $('.ruc-field').val("<%= @provider.ruc %>").change()
  $('.address-field').val("<%= @provider.address %>").change()
  $('.phone-field').val("<%= @provider.phone %>").change()
  $('#provider_information').fadeIn()
  $('select').material_select()
<% end %>